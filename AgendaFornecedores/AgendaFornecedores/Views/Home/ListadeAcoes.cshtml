<!--page-->
@model IEnumerable<AgendaFornecedores.Models.Acao>
@{
    ViewData["Title"] = "Lista de Ações";

}
<h1 class=" text-center display-4">@ViewData["Title"]</h1>

<ul>
@{
    foreach (Acao act in Model)
    {
      <li class="row" > <h2 class="col" >@act.NomeUsuario @act.Action @act.NomeFornecedor @act.Data</h2></li>
       
    }
}
</ul>